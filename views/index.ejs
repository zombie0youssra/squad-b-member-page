<%- include('./partials/head') %>

  <header><img class="logo" src="./logo+background.png" alt="squad B logo">
  
</header>
  

  
  
  <main>

    <nav>
      <ul>
        <li><a href="/?squad=squad-a-2022">Squad A</a></li>
        <li><a href="/?squad=squad-b-2022">Squad B</a></li>
        <li><a href="/?squad=squat-c-2022">Squad C</a></li>
        <!-- <li><a href="/?squad=<%= squad.slug %>&orderBy=name">Sorteer op naam</a></li> -->
    
    </nav>

    <section class="marquee">
      <p>
        <span>
          <strong>new 2023 collection</strong> — FDND edition - <strong>frontenders</strong> — ON SALE —
        </span>
        <span aria-hidden>
          <span>
            <strong>new 2023 collection</strong> — FDND edition - <strong>frontenders</strong> — ON SALE —
          </span>
          <span aria-hidden>
            <span>
              <strong>new 2023 collection</strong> — FDND edition - <strong>frontenders</strong> — ON SALE —
            </span>
            <span aria-hidden>
              <span>
                <strong>new 2023 collection</strong> — FDND edition - <strong>frontenders</strong> — ON SALE —
              </span>
      
      </p>
    </section>

    <!-- <section class="banner">
      <h1 class="collection-year">
        NEW 2023 COLLECTION
      </h1>
    </section> -->




    <section>
      <img class="shoe" src="./orange shoe with shadow.png" alt="ornage jordan high top sneaker">
    </section>

    <!-- members -->
    <% squad.members.forEach(member=> { %>
      <section class="members-in-squad"><article>
        <% if (member.avatar) { %>
          <img src="<%= member.avatar %>" alt="De avatar van <%= member.name %>" />
          <% }else{ %>
            <img src="/placeholder.jfif" alt="Placeholder avatar" />
            <% } %>
              <p>
                <%= member.name %>
                  <%= member.prefix %>
                    <%= member.surname %>
              </p>
      </article>
      </section>
      <% }) %>
  
    <div class="button-container">
      <button>randomize <i class="fa-solid fa-shuffle"></i></button>
      <button>favorite <i class="fa-solid fa-heart"></i></button>
      <button>suprise <i class="fa-solid fa-bomb fa-shake"></i></button>
    </div>
  
  
  </main>


<%- include('./partials/foot') %>
